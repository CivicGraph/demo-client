[
  {
    "selector": "node",
    "style": {
      "label": "data(Body)",
      "background-color": "#666",
      "text-valign": "center",
      "text-halign": "left"
    }
  },
  {
    "selector": "node[obj-class = 'stars']",
    "style": {
      "background-color": "#eeaaee"
    }
  },
  {
    "selector": "node[obj-class = 'planets']",
    "style": {
      "background-color": "#f7a35c"
    }
  },
  {
    "selector": "node[obj-class = 'dwarf_planets']",
    "style": {
      "background-color": "#90ee7e"
    }
  },
  {
    "selector": "node[obj-class = 'moons']",
    "style": {
      "background-color": "#7798BF"
    }
  },
  {
    "selector": "node[obj-class = 'comets']",
    "style": {
      "background-color": "#aaeeee"
    }
  },
  {
    "selector": "node[obj-class = 'asteroids']",
    "style": {
      "background-color": "#ff0066"
    }
  },
  {
    "selector": "node:selected",
    "style": {
      "background-color": "#007bff"
    }
  },
  {
    "selector": "edge",
    "style": {
      "width": 2,
      "line-color": "#ccc",
      "text-rotation": "autorotate",
      "text-margin-y": "-10px",
      "curve-style": "bezier",
      "target-arrow-color": "#ccc",
      "target-arrow-shape": "vee"
    }
  },
  {
    "selector": "edge:selected",
    "style": {
      "line-color": "#007bff",
      "target-arrow-color": "#007bff"
    }
  }
]
